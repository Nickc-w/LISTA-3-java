import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String [ ] array = {"valor1","valor2","valor3","valor1","valor5","valor5","valor7","valor5"};
        String valor;
        int tamanho = 0,ii=0;

        System.out.print("Informe um valor de [1] a [8] (EX: valor1,valor2,valor3...): ");
        valor = sc.next().toLowerCase().trim();

        for (int i = 0;i < 8; i++){
            if (!valor.equals(array[i])) tamanho++;
        }

        String [ ] array2 = new String[tamanho];

            for (int i = 0; i < 8; i++) {
                if (!valor.equals(array[i])) {
                    array2[ii] = array[i];
                    ii++;
                }
            }
        for (int i = 0;i < array2.length; i++) System.out.printf(" %s ",array2[i]);

        sc.close();

    }
}
